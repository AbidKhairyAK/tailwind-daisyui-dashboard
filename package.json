{
	"name": "tailwind-daisyui-dashboard",
	"version": "1.0.0",
	"description": "HTML Admin Dashboard Template built with TailwindCSS and DaisyUI (think of it like Bootstrap for Tailwind)",
	"main": "index.html",
	"scripts": {
		"dev": "tailwindcss -i ./source.css -o ./app.css",
		"watch": "tailwindcss -i ./source.css -o ./app.css --watch",
		"build": "tailwindcss -i ./source.css -o ./app.css --minify",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AbidKhairyAK/tailwind-daisyui-dashboard.git"
	},
	"keywords": [
		"admin template",
		"dashboard template",
		"tailwind template",
		"tailwindcss template",
		"daisyui template",
		"tailwind",
		"tailwindcss",
		"daisyui"
	],
	"author": "Muhammad Abid Khairy",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/AbidKhairyAK/tailwind-daisyui-dashboard/issues"
	},
	"homepage": "https://github.com/AbidKhairyAK/tailwind-daisyui-dashboard#readme",
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.0",
		"@tailwindcss/typography": "^0.5.2",
		"daisyui": "^2.13.1",
		"tailwind-scrollbar": "^1.3.1",
		"tailwindcss": "^3.0.23"
	}
}
